{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tu-delft/image-text",
	"attributes": {
		"showChapterSubtitle": {
			"type": "boolean",
			"default": false
		},
		"chapterSubtitle": {
			"type": "string",
			"default": ""
		},
		"image": {
            "type": "string",
            "default": "/app/themes/tu-delft/src/img/img-placeholder.svg"
        },
        "content": {
            "type": "string",
            "default": ""
        }
	},
	"version": "1.0.0",
	"title": "Image-Text",
	"category": "widgets",
	"icon": "align-pull-left",
	"description": "Image-Text Block for TU-Delft",
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "tu-delft",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js"
}
