{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tu-delft/content-card",
	"attributes": {
		"showChapterSubtitle": {
			"type": "boolean",
			"default": false
		},
		"chapterSubtitle": {
			"type": "string",
			"default": ""
		},
		"links": {
			"type": "array",
			"default": []
		},
		"preview": {
			"type": "boolean",
			"default": false
		}
	},
	"version": "1.0.0",
	"title": "Content Card",
	"category": "widgets",
	"icon": "grid-view",
	"description": "Content Card with links to desired location",
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "tu-delft",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js"
}
