{
  "name": "viamsterdam/tu-delft",
  "type": "project",
  "license": "MIT",
  "description": "TU Delft dashboard",
  "homepage": "https://viamsterdam.com/",
  "authors": [
    {
      "name": "Aljosa Kotur",
      "email": "koturaljosa@gmail.com",
      "homepage": "https://github.com/AljosaK"
    }
  ],
  "keywords": [
    "viamsterdam",
    "tudelft",
    "composer",
    "roots",
    "wordpress",
    "wp",
    "wp-config"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/installers": "^2.2",
    "vlucas/phpdotenv": "^5.5",
    "oscarotero/env": "^2.1",
    "roots/bedrock-autoloader": "^1.0",
    "roots/bedrock-disallow-indexing": "^2.0",
    "roots/wordpress": "6.5.3",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.1.0",
    "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "3.1",
    "wpackagist-plugin/wordpress-seo": "21.9",
    "wpackagist-plugin/svg-support": "2.5.5",
    "humanmade/asset-manager-framework": "^0.13.5",
    "roots/multisite-url-fixer": "^1.1",
    "wpengine/advanced-custom-fields-pro": "^6.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.1",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin",
        "advanced-custom-fields/advanced-custom-fields-pro",
        "wpackagist-plugin/acf-content-analysis-for-yoast-seo",
        "wpackagist-plugin/wordpress-seo",
        "wpackagist-plugin/svg-support"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
